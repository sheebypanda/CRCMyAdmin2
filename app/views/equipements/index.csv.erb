<%- headers = [
  "ip",
  "bp",
  "ville",
  "rattachement",
  "maintenance",
  "sla",
  "coutmaintenance",
  "localisation",
  "adresse",
  "serial",
  "marque",
  "modele",
  "nom",
  "asapid",
  "iosv",
  "datemaintenance",
  "id_localisation",
  "codepostal",
  "etage",
  "tel",
  "mail",
  "description",
  "lat",
  "lng",
  "horaires"] -%>

<%= CSV.generate_line headers %>
<%- @all.each do |eq| -%>
  <% if eq.recette %>
    <% ndi = eq.recette.ligne.ndi %>
    <% internet = eq.recette.ligne.identifiantoperateur %>
    <% loc_id = eq.recette.localisation.id %>
    <% localisation = eq.recette.localisation.nom %>
    <% adresse = eq.recette.localisation.adresse %>
    <% codepostal = eq.recette.localisation.codepostal %>
    <% bp = eq.recette.localisation.bp %>
    <% ville = eq.recette.localisation.ville %>
    <% rattachement = eq.recette.localisation.rattachement %>
  <% end %>
  <% if eq.serial %>
    <% eq.serial.each_line("|") do |s| %>
      <%= CSV.generate_line([
        eq.ip,
        bp,
        ville,
        rattachement,
        eq.maintenance,
        eq.sla,
        eq.coutmaintenance,
        localisation,
        adresse,
        s.tr('|','').strip,
        eq.marque,
        eq.modele,
        eq.nom,
        eq.asapid,
        eq.iosv,
        eq.datemaintenance,
        loc_id,
        codepostal
        ]).html_safe -%>
    <% end %>
  <% end %>
<%- end -%>
