<%- headers = [
  'Marque',
  'Modele',
  'Nom',
  'Numéro de série',
  'Identifiant ASAP',
  'IP',
  'Version IOS',
  'Sortie Internet',
  'Référence opérateur',
  'Localisation',
  'Adresse',
  'CP',
  'Ville',
  'SLA',
  'Equipement maintenu ?',
  'Coût mensuel',
  'Date de début de maintenance'] -%>
<%= CSV.generate_line headers %>
<%- @all.each do |eq| -%>
  <% if eq.recette %>
    <% ndi = eq.recette.ligne.ndi %>
    <% internet = eq.recette.ligne.identifiantoperateur %>
    <% localisation = eq.recette.localisation.nom %>
    <% adresse = eq.recette.localisation.adresse %>
    <% codepostal = eq.recette.localisation.codepostal %>
    <% ville = eq.recette.localisation.ville %>
  <% end %>
  <%= CSV.generate_line([
    eq.marque,
    eq.modele,
    eq.nom,
    eq.serial,
    eq.asapid,
    eq.ip,
    eq.iosv,
    ndi,
    internet,
    localisation,
    adresse,
    codepostal,
    ville,
    eq.sla,
    eq.maintenance,
    eq.coutmaintenance,
    eq.datemaintenance]).html_safe -%>
<%- end -%>
