<%- headers = [
  "marque",
  "modele",
  "nom",
  "serial",
  "asapid",
  "ip",
  "iosv",
  "sla",
  "maintenance",
  "coutmaintenance",
  "datemaintenance",
  "id_localisation",
  "localisation",
  "adresse",
  "codepostal",
  "ville",
  "etage",
  "tel",
  "mail",
  "description",
  "lat",
  "lng",
  "horaires"] -%>
<%= CSV.generate_line headers %>
<%- @all.each do |eq| -%>
  <% if eq.recette %>
    <% ndi = eq.recette.ligne.ndi %>
    <% internet = eq.recette.ligne.identifiantoperateur %>
    <% loc_id = eq.recette.localisation.id %>
    <% localisation = eq.recette.localisation.nom %>
    <% adresse = eq.recette.localisation.adresse %>
    <% codepostal = eq.recette.localisation.codepostal %>
    <% ville = eq.recette.localisation.ville %>
  <% end %>
<%= CSV.generate_line([
  eq.marque,
  eq.modele,
  eq.nom,
  eq.serial,
  eq.asapid,
  eq.ip,
  eq.iosv,
  eq.sla,
  eq.maintenance,
  eq.coutmaintenance,
  eq.datemaintenance,
  loc_id,
  localisation,
  adresse,
  codepostal,
  ville]).html_safe -%>
<%- end -%>
