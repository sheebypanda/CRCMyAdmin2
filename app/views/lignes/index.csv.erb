<%- headers = [
  "operateur_id",
  "techno",
  "numerocompte",
  "ndi",
  "ippublique",
  "identifiantoperateur",
  "mdpoperateur",
  "debit",
  "vlan",
  "cout",
  'nom site',
  'adresse',
  'cp',
  'ville'] -%>
<%= CSV.generate_line headers %>
<%- @li.each do |li| -%>
  <% if li.recettes.first %>
    <% nom = li.recettes.first.localisation.nom %>
    <% adresse = li.recettes.first.localisation.adresse %>
    <% cp = li.recettes.first.localisation.codepostal %>
    <% ville = li.recettes.first.localisation.ville %>
  <% end %>
<%= CSV.generate_line([
  li.operateur_id,
  li.techno,
  li.numerocompte,
  li.ndi,
  li.ippublique,
  li.identifiantoperateur,
  li.mdpoperateur,
  li.debit,
  li.vlan,
  li.cout,
  nom,
  adresse,
  cp,
  ville
  ]).html_safe -%>
<%- end -%>
