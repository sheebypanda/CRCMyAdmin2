<%- headers = [
  "ville",
  "nom",
  "adresse",
  "codepostal",
  "etage",
  "tel",
  "mail",
  "description",
  "horaires",
  "lat",
  "lng"
] -%>
<%= CSV.generate_line headers %>
<%- @lo.each do |lo| -%>
<%= CSV.generate_line([
  lo.ville,
  lo.nom,
  lo.adresse,
  lo.codepostal,
  lo.etage,
  lo.tel,
  lo.mail,
  lo.description,
  lo.horaires,
  lo.lat,
  lo.lng
  ]).html_safe -%>
<%- end -%>
